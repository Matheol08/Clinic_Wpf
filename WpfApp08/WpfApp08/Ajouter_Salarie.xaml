        <Button Click="Ajouter_Click" Content="Ajouter" HorizontalAlignment="Left" Margin="204,497,0,0" VerticalAlignment="Top" Height="28" Width="66" Foreground="#FF080101" Background="#FF3DDDC2" BorderBrush="#FF3DDDC2"/>
