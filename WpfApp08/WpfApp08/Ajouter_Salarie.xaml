        <TextBox x:Name="Text2" HorizontalAlignment="Left" Margin="155,124,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" IsReadOnly="false"/>
        <TextBox x:Name="Text3" HorizontalAlignment="Left" Margin="155,186,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" IsReadOnly="false"/>
        <TextBox x:Name="Text4" HorizontalAlignment="Left" Margin="155,252,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" IsReadOnly="false"/>
